@BOOK{Kay2018101,
	author = {Kay, Martin},
	title = {Putting linguistics back into computational linguistics},
	year = {2018},
	journal = {Language, Cognition, and Computational Models},
	pages = {101 – 117},
	doi = {10.1017/9781316676974.005},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85048384780&doi=10.1017%2f9781316676974.005&partnerID=40&md5=83d2f8d89a4a00852737f5f2d3c3052a},
	affiliations = {Department of Linguistics, Stanford University, United States},
	abstract = {Almost all practitioners of natural language processing make a crucial error that also besets much of Chomsky's argument about the poverty of the stimulus in first language learning, namely that we can discover all we need to know about language by examining sufficiently large quantities of it. The error is to ignore the crucial function of language in referring to things in the real and imaginary worlds. Speakers and hearers appeal to this in many ways. Translators rely on it to provide information that they must supply in the target text but which is only implicit in the source. Reference is one of several properties of language that relate parts of a text or a discourse that may not be adjacent to one another. To the extent that linguists are concerned with how people use language for communication, they are interested in processes in people's heads and, to the extent that they are concerned with processes, they are interested in computation. It is this, rather that engineering feats like machine translation, that gives computational linguistics its special role. © Cambridge University Press 2017.},
	keywords = {Computational linguistics; Natural language processing systems; Crucial functions; In-process; Language learning; Machine translations; NAtural language processing; Real and imaginary; Translation (languages)},
	publisher = {Cambridge University Press},
	isbn = {978-131667697-4; 978-110716222-8},
	language = {English},
	abbrev_source_title = {Language, Cognition, and Computational Models},
	type = {Book chapter},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 0}
}


	type = {Conference paper},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 7; Conference name: AISB 2008 Symposium on Computing and Philosophy; Conference date: 1 April 2008 through 4 April 2008; Conference code: 89215}
}


	keywords = {Calculations; Chemical analysis; Maximum likelihood; Mining engineering; Poisson distribution; Production; Regression analysis; Students; Basic cycle; Control and automation; Experimental physics; Generalized linear model; Performance measurements; Poisson regression; Poisson regression models; Proportional relationships; Risk assessment},

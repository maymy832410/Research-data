@ARTICLE{Heide20222533,
	author = {Heide, Klaas Maximilian and Heikebrügge, Steffen and Dänekas, Christian and Breidenstein, Bernd and Schaumann, Peter},
	title = {Automated geometry measurement and deep rolling of butt welds},
	year = {2022},
	journal = {Welding in the World},
	volume = {66},
	number = {12},
	pages = {2533 – 2547},
	doi = {10.1007/s40194-022-01346-w},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85135075052&doi=10.1007%2fs40194-022-01346-w&partnerID=40&md5=5f0b8a736097c1aabf17ce5c77114ebf},
	affiliations = {Institute of Production Engineering and Machine Tools, Leibniz University Hannover, An der Universität 2, Garbsen, 30823, Germany; Institute for Steel Construction, Leibniz University Hannover, ForWind, Appelstrasse 9A, Hannover, 30167, Germany},
	abstract = {During the joining of two metal sheets by welding, a process-specific geometry of the weld is created. The local geometry of the created weld has a decisive influence on its fatigue strength. This is due to stress concentration at the geometric notches. In this paper, a process known from mechanical engineering called deep rolling is applied on butt welds. The influence on the local weld geometry and the local stress concentration after deep rolling is investigated. Additionally, a novel automated measurement system using optical laser line scanning is presented. The system is qualified for the evaluation of the local weld geometry regarding its flank angles and toe radii. The presented investigations show that the deep rolling process influences the stress concentrations determined by 2D-FE-simulations using real scan data. A correlation between the difference in toe radii or local notch stresses before and after deep rolling and the initial flank angle was found. This indicates that there are process and geometry specific conditions for the successful application of the deep rolling process. © 2022, The Author(s).},
	author_keywords = {Automation; Butt welds; Deep rolling; Laser scanning; Local notch geometry; Post-treatment},
	keywords = {Butt welding; Geometry; Stress concentration; Welds; Butt weld; Deep rolling; Geometry measurements; Laser scanning; Local geometry; Local notch geometry; Notch geometry; Post treatment; Rolling process; Weld geometry; Automation},
	correspondence_address = {C. Dänekas; Institute for Steel Construction, Leibniz University Hannover, Hannover, ForWind, Appelstrasse 9A, 30167, Germany; email: daenekas@stahl.uni-hannover.de},
	publisher = {Springer Science and Business Media Deutschland GmbH},
	issn = {00432288},
	coden = {WDWRA},
	language = {English},
	abbrev_source_title = {Weld. World},
	type = {Article},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 2; All Open Access, Hybrid Gold Open Access}
}

